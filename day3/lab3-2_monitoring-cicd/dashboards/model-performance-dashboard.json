{
  "dashboard": {
    "title": "ML Model Performance Dashboard",
    "tags": ["ml", "monitoring", "ab-test"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Model MAE Score (Real-time)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "model_mae_score{model_name=\"california-housing\", version=\"v1.0\"}",
            "legendFormat": "Model A (v1.0)",
            "refId": "A"
          },
          {
            "expr": "model_mae_score{model_name=\"california-housing\", version=\"v2.0\"}",
            "legendFormat": "Model B (v2.0)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "MAE Score", "format": "short"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"},
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "pointradius": 2,
        "points": false,
        "tooltip": {"shared": true, "sort": 0, "value_type": "individual"},
        "legend": {"show": true, "values": true, "current": true, "avg": true},
        "thresholds": [
          {
            "value": 0.40,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 2,
        "title": "Model R² Score",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "model_r2_score{model_name=\"california-housing\", version=\"v1.0\"}",
            "legendFormat": "Model A (v1.0)",
            "refId": "A"
          },
          {
            "expr": "model_r2_score{model_name=\"california-housing\", version=\"v2.0\"}",
            "legendFormat": "Model B (v2.0)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "R² Score", "format": "percentunit", "max": 1, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Prediction Latency (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_prediction_latency_bucket{version=\"v1.0\"}[5m]))",
            "legendFormat": "Model A (P95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(model_prediction_latency_bucket{version=\"v2.0\"}[5m]))",
            "legendFormat": "Model B (P95)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "Latency (seconds)", "format": "s"},
          {"format": "short"}
        ],
        "thresholds": [
          {
            "value": 0.1,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "Requests per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(model_prediction_total{status=\"success\", version=\"v1.0\"}[1m])",
            "legendFormat": "Model A (Success)",
            "refId": "A"
          },
          {
            "expr": "rate(model_prediction_total{status=\"success\", version=\"v2.0\"}[1m])",
            "legendFormat": "Model B (Success)",
            "refId": "B"
          },
          {
            "expr": "rate(model_prediction_total{status=\"error\"}[1m])",
            "legendFormat": "Errors",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"label": "RPS", "format": "ops"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Traffic Distribution (A/B Test)",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(model_prediction_total{version=\"v1.0\"})",
            "legendFormat": "Model A (v1.0)",
            "refId": "A"
          },
          {
            "expr": "sum(model_prediction_total{version=\"v2.0\"})",
            "legendFormat": "Model B (v2.0)",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]},
          "pieType": "donut"
        }
      },
      {
        "id": 6,
        "title": "Model Accuracy Score",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "model_accuracy_score{model_name=\"california-housing\", version=\"v1.0\"}",
            "refId": "A"
          },
          {
            "expr": "model_accuracy_score{model_name=\"california-housing\", version=\"v2.0\"}",
            "refId": "B"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.75, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Alert Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "ALERTS{alertname=~\"ModelPerformanceDegraded|ModelLatencyHigh\"}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "options": {"0": {"text": "OK", "color": "green"}}},
              {"type": "value", "options": {"1": {"text": "ALERT", "color": "red"}}}
            ]
          }
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(model_prediction_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Errors/sec", "format": "ops"},
          {"format": "short"}
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {"params": [0.01], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "message": "Model error rate exceeded 1%",
          "noDataState": "no_data"
        }
      },
      {
        "id": 9,
        "title": "Model Performance Comparison",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "model_mae_score{model_name=\"california-housing\"}",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "model_r2_score{model_name=\"california-housing\"}",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(model_prediction_latency_bucket[5m]))",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "job": true, "instance": true},
              "indexByName": {"version": 0, "Value #A": 1, "Value #B": 2, "Value #C": 3},
              "renameByName": {
                "version": "Model Version",
                "Value #A": "MAE",
                "Value #B": "R² Score",
                "Value #C": "P95 Latency"
              }
            }
          }
        ]
      }
    ],
    "time": {"from": "now-30m", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
